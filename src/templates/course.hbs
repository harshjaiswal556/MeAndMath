  <script>
    let search = document.getElementById("searchTxt");
search.addEventListener("input",function(){
    let inputval=search.value;
    let noteCards = document.getElementsByClassName('card');
    Array.from(noteCards).forEach(function(element,index){
      console.log(index)
        let cardTxt = document.getElementsByClassName("card-text")[index].innerHTML;
        let cardTtl = document.getElementsByClassName("card-title")[index].innerHTML;
        if((cardTxt.toLowerCase()).includes(inputval.toLowerCase()) || (cardTtl.toLowerCase()).includes(inputval.toLowerCase())){
            element.style.display = "block";
        }
        else{
            element.style.display = "none";

        }
    });
});
